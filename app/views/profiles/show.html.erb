<div class="title_section">
	<h1 align="center">Tu perfil personal - <%= @profile.full_name %></h1>
</div>
<div class="panel panel-default">
<div class="panel-heading">
  	<h3>Datos personales</h3>
  </div>
  <div class="panel-body">
  	<div class="row">
	  	<div class="col-md-1"><%= image_tag("avatar.png",size: "100x100", alt: "icon avatar") %></div>
	  	<div class="col-md-6"><%= react_component 'Profile', {data: ProfileSerializer.new(@profile).attributes} %></div>
  	</div>
	</div>
	<div class="btn-group" role="group" >
		<button type="button" class="btn btn-default"><%= link_to "Inicio", authenticated_root_path %></button>
	</div>
</div>